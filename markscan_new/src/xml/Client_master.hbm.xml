<!-- <!DOCTYPE hibernate-mapping PUBLIC
          "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"> -->
          <!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.markscan.project.beans.Client_master" table="client_master">
		<id name="id">
			<generator class="increment"></generator>
		</id>

		<property name="created_time" />
		<property name="client_name" />
		<property name="client_address" />
		<property name="client_subject" />
		<property name="project_type" />
		<property name="email_module" />
		<property name="created_by" />
		<property name="client_email" />
		
	</class>

</hibernate-mapping>