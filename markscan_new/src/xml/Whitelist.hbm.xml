          <!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.markscan.project.beans.Whitelist"
		table="whitelist">
		<id name="id">
			<generator class="increment"></generator>
		</id>

		<property name="domain_name"></property>
		<property name="clientId"></property>
		<property name="created_on"></property>
		<property name="project_type"></property>
		<property name="created_by"></property>
		
	</class>

</hibernate-mapping>